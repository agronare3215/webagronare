# env.example - copy to .env and fill secrets (DO NOT commit .env)
PORT=3000

# Google Maps JS key (client-side). Restrict by HTTP referrers in Cloud Console.
GOOGLE_MAPS_KEY=YOUR_GOOGLE_MAPS_KEY

# Server-side API keys (never expose in the client)
GEMINI_API_KEY=YOUR_GEMINI_KEY
SENDGRID_API_KEY=
SMTP_URL=      # example: smtp://user:pass@smtp.host:587

# Email from address
FROM_EMAIL=no-reply@agronare.com

# Puppeteer: optional path if using host chromium binary
PUPPETEER_EXECUTABLE_PATH=
